{
  "cli": {
    "version": ">= 5.9.1",
      "appVersionSource": "local"
  },
  "build": {
    "preview": {
      "ios": {
        "simulator": true
      }
    },
    "preview2": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "preview3": {
      "developmentClient": true
    },
    "development": {
      "ios": {
        "resourceClass": "medium",
        "developmentClient": true,
        "env": {
          "EXPO_PUBLIC_APPWRITE_ENDPOINT": "https://fra.cloud.appwrite.io/v1",
          "EXPO_PUBLIC_APPWRITE_PROJECT_ID": "695d3166000489e7b728"
        }
      }
    },
    "production": {
      "ios": {
        "resourceClass": "medium",
        "env": {
          "EXPO_PUBLIC_APPWRITE_ENDPOINT": "https://fra.cloud.appwrite.io/v1",
          "EXPO_PUBLIC_APPWRITE_PROJECT_ID": "695d3166000489e7b728"
        }
      }
    },
    "testflight": {
      "ios": {
        "resourceClass": "medium",
        "developmentClient": false,
        "env": {
          "EXPO_PUBLIC_APPWRITE_ENDPOINT": "https://fra.cloud.appwrite.io/v1",
          "EXPO_PUBLIC_APPWRITE_PROJECT_ID": "695d3166000489e7b728"
        }
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6756985580",
        "groups": ["External Testers"]
      }
    },
    "testflight": {
      "ios": {
        "ascAppId": "6756985580"
      }
    }
  }
}
