{
  "cli": {
    "version": ">= 5.9.1"
  },
  "build": {
    "preview": {
      "ios": {
        "simulator": true
      }
    },
    "preview2": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "preview3": {
      "developmentClient": true
    },
    "production": {
      "ios": {
        "resourceClass": "medium"
      }
    },
    "testflight": {
      "ios": {
        "resourceClass": "medium",
        "env": {
          "EXPO_PUBLIC_APPWRITE_PROJECT_ID": "69455c28002093139de1",
          "EXPO_PUBLIC_APPWRITE_PROJECT_NAME": "budget-app",
          "EXPO_PUBLIC_APPWRITE_ENDPOINT": "https://fra.cloud.appwrite.io/v1",
          "EXPO_PUBLIC_APPWRITE_DATABASE_ID": "69455cde003a052c1e1d",
          "EXPO_PUBLIC_APPWRITE_TABLE_TRANSACTIONS": "transactions",
          "EXPO_PUBLIC_APPWRITE_TABLE_BUDGETS": "budgets",
          "EXPO_PUBLIC_APPWRITE_TABLE_CATEGORIES": "categories",
          "EXPO_PUBLIC_APPWRITE_TABLE_USERS": "users",
          "EXPO_PUBLIC_APPWRITE_TABLE_BALANCES": "balances",
          "EXPO_PUBLIC_APPWRITE_TABLE_MERCHANT_VOTES": "merchant_votes"
        }
      },
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6756985580",
        "groups": ["External Testers"]
      }
    },
    "testflight": {
      "ios": {
        "ascAppId": "6756985580",
        "groups": ["External Testers"]
      }
    }
  }
}
